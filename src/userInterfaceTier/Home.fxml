<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="userLogicTier.HomeController">
    <stylesheets>
        <URL value="@styles.css" />
    </stylesheets>
   <children>
      <BorderPane layoutY="1.0" prefHeight="400.0" prefWidth="600.0">
         <top>
            <StackPane prefHeight="98.0" prefWidth="600.0" styleClass="vBox" BorderPane.alignment="CENTER">
               <children>
                  <HBox prefHeight="49.0" prefWidth="600.0">
                     <children>
                        <Text id="lblWelcome" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="lblTitle" text="Welcome" />
                        <Button id="btnLogOut" mnemonicParsing="false" styleClass="btnSignIn" text="LogOut" />
                     </children>
                  </HBox>
               </children>
            </StackPane>
         </top>
         <left>
            <StackPane prefHeight="150.0" prefWidth="200.0" styleClass="vBox" BorderPane.alignment="CENTER">
               <children>
                  <VBox prefHeight="219.0" prefWidth="135.0">
                     <children>
                        <ImageView id="imgProfile" fitHeight="99.0" fitWidth="102.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../resources/avatar.png" />
                           </image>
                           <clip>
                               <Circle centerX="50" centerY="50" radius="50" />
                           </clip>
                        </ImageView>
                        <Text id="lblName" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="lblTitle" text="name" />
                     </children></VBox>
               </children>
            </StackPane>
         </left>
         <center>
            <StackPane prefHeight="150.0" prefWidth="200.0" styleClass="vBox" BorderPane.alignment="CENTER">
               <children>
                  <VBox prefHeight="219.0" prefWidth="473.0">
                     <children>
                        <ScrollPane prefHeight="200.0" prefWidth="200.0">
                          <content>
                            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0">
                                 <children>
                                    <VBox prefHeight="200.0" prefWidth="346.0">
                                       <children>
                                          <Text id="lblEmail" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="lblInput" text="Email" />
                                          <TextField id="txtfieldEmail" promptText="example@email.com" />
                                          <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Address" />
                                          <HBox prefHeight="100.0" prefWidth="200.0">
                                             <children>
                                                <VBox prefHeight="200.0" prefWidth="100.0">
                                                   <children>
                                                      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Street" />
                                                      <TextField id="txtfieldStreet" promptText="example Street" />
                                                   </children>
                                                </VBox>
                                                <VBox prefHeight="200.0" prefWidth="100.0">
                                                   <children>
                                                      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="City" />
                                                      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="example City" />
                                                   </children>
                                                </VBox>
                                                <VBox prefHeight="200.0" prefWidth="100.0">
                                                   <children>
                                                      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="ZIP" />
                                                      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="example ZIP" />
                                                   </children>
                                                </VBox>
                                             </children>
                                          </HBox>
                                       </children>
                                    </VBox>
                                 </children></AnchorPane>
                          </content>
                        </ScrollPane>
                     </children></VBox>
               </children>
            </StackPane>
         </center>
         <bottom>
            <StackPane prefHeight="82.0" prefWidth="600.0" styleClass="vBox" BorderPane.alignment="CENTER">
               <children>
                  <HBox prefHeight="100.0" prefWidth="200.0" />
               </children>
            </StackPane>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
